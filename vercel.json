{
  "version": 2,
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "nextjs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "KIMI_API_KEY": "@kimi_api_key",
    "NEXT_PUBLIC_API_URL": "@public_api_url"
  }
} 